# Google AI API Configuration
# Get your API key from: https://aistudio.google.com/app/apikey
GOOGLE_API_KEY=your_google_api_key_here

# Freqtrade API Configuration
# URL where your Freqtrade instance is running
FREQTRADE_API_URL=http://127.0.0.1:8081

# Freqtrade API Credentials
# These should match the credentials in your freqtrade/adk_config.json
FREQTRADE_USERNAME=Freqtrader # This should match the username in your freqtrade/adk_config.json
FREQTRADE_PASSWORD=SuperSecret1! # This should match the password in your freqtrade/adk_config.json

# TLS Verification
# Set to 'false' for local development, 'true' for production
FREQTRADE_VERIFY_TLS=false

GOOGLE_GENAI_USE_VERTEXAI=FALSE

NVIDIA_NIM_API_KEY=your_nvidia_api_key_here
OPENROUTER_API_KEY=your_openrouter_api_key_here
NVIDIA_NIM_API_BASE=https://integrate.api.nvidia.com/v1
OPENROUTER_API_BASE=https://openrouter.ai/api/v1

